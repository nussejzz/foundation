        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="RSA Encrypt Password → encryptedPassword">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="script">
import java.security.KeyFactory
import java.security.spec.X509EncodedKeySpec
import javax.crypto.Cipher
import java.util.Base64

def publicKeyRaw = vars.get(&quot;publicKeyRaw&quot;)
def password = vars.get(&quot;password&quot;)

if (!publicKeyRaw || publicKeyRaw == &quot;NOT_FOUND&quot;) {
    log.error(&quot;❌ RSA Public Key NOT FOUND from server&quot;)
    throw new IllegalStateException(&quot;publicKeyRaw NOT_FOUND&quot;)
}

def keyText = publicKeyRaw.replace(&quot;-----BEGIN PUBLIC KEY-----&quot;,&quot;&quot;)
                          .replace(&quot;-----END PUBLIC KEY-----&quot;,&quot;&quot;)
                          .replaceAll(&quot;\\s+&quot;,&quot;&quot;)
def keyBytes = Base64.getDecoder().decode(keyText)
def keySpec = new X509EncodedKeySpec(keyBytes)
def pubKey = KeyFactory.getInstance(&quot;RSA&quot;).generatePublic(keySpec)

def cipher = Cipher.getInstance(&quot;RSA/ECB/PKCS1Padding&quot;)
cipher.init(Cipher.ENCRYPT_MODE, pubKey)
def encryptedPassword = Base64.getEncoder().encodeToString(cipher.doFinal(password.getBytes(&quot;UTF-8&quot;)))

vars.put(&quot;encryptedPassword&quot;, encryptedPassword)
log.info(&quot;✅ Encrypted password generated: len=&quot; + encryptedPassword.length())
          </stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">false</stringProp>
