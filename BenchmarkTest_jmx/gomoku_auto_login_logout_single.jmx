        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="RSA Encrypt Password -&gt; ${encryptedPassword}">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="script">
import java.security.KeyFactory
import java.security.spec.X509EncodedKeySpec
import javax.crypto.Cipher
import java.util.Base64

// Read variables
String publicKeyRaw = vars.get(&quot;publicKeyRaw&quot;)
String password = vars.get(&quot;password&quot;)

if (publicKeyRaw == null || publicKeyRaw == &quot;NOT_FOUND&quot;) {
    log.error(&quot;publicKeyRaw not found from API response.&quot;)
    throw new IllegalStateException(&quot;publicKeyRaw NOT_FOUND&quot;)
}
if (password == null) {
    log.error(&quot;password variable is null&quot;)
    throw new IllegalStateException(&quot;password variable is null&quot;)
}

// Clean PEM header/footer and whitespace
String cleaned = publicKeyRaw.trim()
cleaned = cleaned.replace(&quot;-----BEGIN PUBLIC KEY-----&quot;,&quot;&quot;).replace(&quot;-----END PUBLIC KEY-----&quot;,&quot;&quot;)
cleaned = cleaned.replaceAll(&quot;\\s+&quot;,&quot;&quot;)

byte[] keyBytes = Base64.getDecoder().decode(cleaned)
X509EncodedKeySpec spec = new X509EncodedKeySpec(keyBytes)
KeyFactory kf = KeyFactory.getInstance(&quot;RSA&quot;)
def pubKey = kf.generatePublic(spec)

Cipher cipher = Cipher.getInstance(&quot;RSA/ECB/PKCS1Padding&quot;)
cipher.init(Cipher.ENCRYPT_MODE, pubKey)
byte[] enc = cipher.doFinal(password.getBytes(&quot;UTF-8&quot;))
String encryptedBase64 = Base64.getEncoder().encodeToString(enc)

vars.put(&quot;encryptedPassword&quot;, encryptedBase64)
log.info(&quot;Encrypted password generated. len=&quot; + encryptedBase64.length())
          </stringProp>
        </JSR223Sampler>
